public static class LLToStackAdapter {
    LinkedList<Integer> list;

    public LLToStackAdapter() {
      list = new LinkedList<>();
    }


    int size() {
      // write your code here
      return list.size();
    }

    void push(int val) {
      // write your code here
      list.addLast(val);
    }

    int pop() {
      // write your code here
      return list.removeLast();
    }

    int top() {
      // write your code here
      return list.getLast();
    }
  }
